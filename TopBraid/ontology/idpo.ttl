  rdfs:range :InformationDelivery ;
  rdfs:subPropertyOf owl:topObjectProperty ;
  owl:inverseOf :hasRequirement ;
  skos:prefLabel "is information requirement of" ;
.
:isSpecificationOf
  rdf:type owl:ObjectProperty ;
  rdfs:domain :InformationDeliverySpecification ;
  rdfs:label "is delivery specification of" ;
