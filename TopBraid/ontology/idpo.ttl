<http://www.inf.bi.rub.de/semweb/ns/information-delivery-process-ontology/idpo>
  rdf:type owl:Ontology ;
  dc:creator [
      rdf:type foaf:Person ;
      foaf:name "Philipp Hagedorn" ;
    ] ;
  dc:description "The inked Building Data Information Delivery Ontology (LID) is defined for ..." ;
  dc:title "LBD Information Delivery Ontology (LID)" ;
  owl:imports dc: ;
  owl:imports <http://purl.org/dc/terms/> ;
  owl:imports <http://www.reiter.at/ontology/bpmn2.0> ;
  owl:imports <http://www.w3.org/ns/prov-o#> ;
  owl:versionInfo "Created with TopBraid Composer" ;
.
:InformationDelivery
  rdf:type owl:Class ;
  rdfs:label "InformationDelivery" ;
  rdfs:subClassOf bpmn2:Task ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf prov:Activity ;
.
:InformationDeliverySpecification
  rdf:type owl:Class ;
  rdfs:label "Information delivery specification" ;
  rdfs:subClassOf owl:Thing ;
  skos:prefLabel "Information delivery specification" ;
.
:InformationObject
  rdf:type owl:Class ;
  rdfs:label "InformationObject" ;
  rdfs:subClassOf bpmn2:DataObject ;
  rdfs:subClassOf owl:Thing ;
  rdfs:subClassOf prov:Entity ;
.
:InformationRequirement
  rdf:type owl:Class ;
  rdfs:label "InformationRequirement" ;
  rdfs:subClassOf owl:Thing ;
.
:InformationStatus
  rdf:type owl:Class ;
  rdfs:label "InformationStatus" ;
  rdfs:subClassOf owl:Thing ;
.
:Project
  rdf:type owl:Class ;
  rdfs:label "Project" ;
  rdfs:subClassOf owl:Thing ;
  skos:prefLabel "Project" ;
.
:deliverySpecification
  rdf:type owl:ObjectProperty ;
  rdfs:domain :InformationDeliverySpecification ;
  rdfs:label "delivery specification" ;
  rdfs:subPropertyOf owl:topObjectProperty ;
  skos:prefLabel "delivery specification" ;
.
:generatedByDelivery
  rdf:type owl:ObjectProperty ;
  rdfs:domain :InformationObject ;
  rdfs:range :InformationDelivery ;
  rdfs:subPropertyOf owl:topObjectProperty ;
  rdfs:subPropertyOf prov:wasGeneratedBy ;
  owl:inverseOf :generatesObject ;
.
:generatesObject
  rdf:type owl:ObjectProperty ;
  rdfs:domain :InformationDelivery ;
  rdfs:range :InformationObject ;
  rdfs:subPropertyOf owl:topObjectProperty ;
  rdfs:subPropertyOf prov:generated ;
  owl:inverseOf :generatedByDelivery ;
.
:hasData
  rdf:type owl:ObjectProperty ;
  rdfs:domain :InformationObject ;
  rdfs:label "has data" ;
  rdfs:subPropertyOf owl:topObjectProperty ;
  rdfs:subPropertyOf prov:atLocation ;
  skos:prefLabel "has data" ;
.
:hasRequirement
  rdf:type owl:ObjectProperty ;
  rdfs:domain :InformationDelivery ;
  rdfs:range :InformationRequirement ;
  rdfs:subPropertyOf owl:topObjectProperty ;
  owl:inverseOf :isRequirementOf ;
.
:hasSpecification
  rdf:type owl:ObjectProperty ;
  rdfs:domain :InformationObject ;
  rdfs:range :InformationDeliverySpecification ;
  rdfs:subPropertyOf owl:topObjectProperty ;
  owl:inverseOf :isSpecificationOf ;
.
:hasStatus
  rdf:type owl:ObjectProperty ;
  rdfs:domain :InformationObject ;
  rdfs:label "has status" ;
