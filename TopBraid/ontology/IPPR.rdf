&lt;xsd:attribute name=&quot;bpmnElement&quot; type=&quot;xsd:QName&quot;/&gt;
&lt;/xsd:extension&gt;
&lt;/xsd:complexContent&gt;
&lt;/xsd:complexType&gt;</rdfs:comment>
