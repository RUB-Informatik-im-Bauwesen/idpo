                <owl:onClass rdf:resource="http://www.reiter.at/ontology/bpmn2.0#SequenceFlow"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.reiter.at/ontology/bpmn2.0#default"/>
