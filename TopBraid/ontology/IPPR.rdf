                <owl:onClass rdf:resource="http://www.reiter.at/ontology/bpmn2.0#UserTask"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.reiter.at/ontology/bpmn2.0#userTask"/>
                <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.reiter.at/ontology/bpmn2.0#GlobalUserTask"/>
