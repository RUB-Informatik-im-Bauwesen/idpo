    <owl:Class rdf:about="http://www.reiter.at/ontology/bpmn2.0#InclusiveGateway">
        <rdfs:subClassOf rdf:resource="http://www.reiter.at/ontology/bpmn2.0#AnalyticProcessModelingConformance"/>
