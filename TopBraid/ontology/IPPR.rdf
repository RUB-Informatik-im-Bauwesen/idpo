    <!-- http://www.reiter.at/ontology/bpmn2.0#DataAssociation -->

    <owl:Class rdf:about="http://www.reiter.at/ontology/bpmn2.0#DataAssociation">
        <rdfs:subClassOf rdf:resource="http://www.reiter.at/ontology/bpmn2.0#CommonExecutableProcessModelingConformance"/>
        <rdfs:subClassOf rdf:resource="http://www.reiter.at/ontology/bpmn2.0#Data"/>
