     xml:base="http://www.reiter.at/ontology/bpmn2.0"
     xmlns:bpmn="http://www.reiter.at/ontology/bpmn2.0#bpmn:"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:bpmn2="http://www.wageneder.at/ontology/bpmn2.0#"
     xmlns:dc="http://purl.org/dc/elements/1.1/">
    <owl:Ontology rdf:about="http://www.reiter.at/ontology/bpmn2.0">
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">BPMN 2.0 Ontology - created as part of the IPPR project at the Master Degree Course &quot;Information Management&quot; at the University of Applied Sciences in Graz, Austria.</rdfs:comment>
        <dc:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Martina Reiter</dc:creator>
    </owl:Ontology>
    


